{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es6"
        ],
        // "strictNullChecks": true,
        //"noImplicitAny": true,
        // "noUnusedLocals": true,
        "sourceMap": true,
        //"emitDecoratorMetadata": true,
        //"experimentalDecorators": true,
        "rootDir": ".",
        // "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        // "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true
        
    },
    "exclude": [
        "node_modules"
    ]
}
